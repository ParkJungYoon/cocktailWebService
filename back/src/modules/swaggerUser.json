{
  "paths": {
    "/": {
      "get": {
        "tags": ["Main"],
        "summary": "Main Page Get",
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/register": {
      "post": {
        "tags": ["User"],
        "summary": "register user Post",
        "parameters": [
          {
            "in": "body",
            "name": "body",
            "email": "body",
            "password": "body",
            "schema": {
              "$ref": "#/definitions/User"
            }
          }
        ],
        "responses": {
          "201": {
            "description": "OK"
          },
          "404": {
            "description": "Failed. Bad Request"
          }
        }
      }
    },
    "/login/modify": {
      "post": {
        "tags": ["User"],
        "summary": "회원 정보 수정",
        "parameters": [
          {
            "name": "authorization",
            "in": "header",
            "description": "JWT access token",
            "required": true,
            "type": "string"
          },
          {
            "name": "body",
            "in": "body",
            "email": "body",
            "password": "body",
            "schema": {
              "$ref": "#/definitions/User"
            }
          }
        ],
        "responses": {
          "201": {
            "description": "OK"
          },
          "404": {
            "description": "Failed. Bad Request"
          }
        }
      }
    },
    "/login": {
      "post": {
        "tags": ["User"],
        "summary": "로그인",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "email": "body",
            "password": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/User2"
            }
          }
        ],
        "responses": {
          "201": {
            "description": "OK"
          },
          "404": {
            "description": "Failed. Bad Request"
          }
        }
      }
    },
    "/auth/google": {
      "get": {
        "tags": ["User"],
        "summary": "구글 login",
        "description": "해당 path로 로그인 후엔 /login로 돌아갑니다.",
        "responses": {
          "201": {
            "description": "OK"
          },
          "404": {
            "description": "Failed. Bad Request"
          }
        }
      }
    },
    "/refresh": {
      "get": {
        "tags": ["User"],
        "summary": ["Access Token 재발급"],
        "description": "access token은 토큰 앞에 'Bearer ' 꼭 붙여서 request 해야합니다.",
        "parameters": [
          {
            "name": "authorization",
            "in": "header",
            "description": "JWT access token",
            "required": true,
            "type": "string"
          },
          {
            "name": "refresh",
            "in": "header",
            "description": "JWT refresh token",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "201": {
            "description": "OK"
          },
          "404": {
            "description": "Failed. Bad Request"
          }
        }
      }
    },
    "/user/current": {
      "get": {
        "tags": ["User"],
        "summary": ["토큰 유효성 검사, 토큰 속 userId 정보 반환"],
        "description": "access token은 토큰 앞에 'Bearer ' 꼭 붙여서 request 해야합니다. 토큰 속 user 정보를 반환합니다.",
        "parameters": [
          {
            "name": "authorization",
            "in": "header",
            "description": "JWT access token",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "201": {
            "description": "OK"
          },
          "404": {
            "description": "Failed. Bad Request"
          }
        }
      }
    }
